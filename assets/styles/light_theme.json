{
    "QWidget": {
        "background-color": "#ffffff",
        "color": "#000000"
    },
    "QPushButton": {
        "background-color": "#e0e0e0",
        "border": "1px solid #cccccc",
        "padding": "10px",
        "border-radius": "5px"
    },
    "QPushButton:hover": {
        "background-color": "#d0d0d0"
    },
    "QLineEdit": {
        "background-color": "#f5f5f5",
        "border": "1px solid #cccccc",
        "padding": "5px",
        "border-radius": "3px",
        "color": "#000000"
    },
    "QLineEdit::placeholder": {
        "color": "#888888"
    },
    "QTableWidget": {
        "background-color": "#f5f5f5",
        "color": "#000000"
    },
    "QTableWidget::item": {
        "padding": "10px"
    },
    "QLabel": {
        "color": "#000000"
    },
    "QHeaderView::section": {
        "background-color": "#e0e0e0",
        "color": "#000000",
        "padding": "5px",
        "border": "1px solid #cccccc",
        "font-weight": "bold"
    },
    "TitleBar": {
        "background-color": "#cccccc"
    },
    "TitleBar QPushButton": {
        "background-color": "#bbbbbb",
        "color": "#000000",
        "border": "none",
        "font-size": "16px"
    },
    "TitleBar QPushButton:hover": {
        "background-color": "#aaaaaa"
    }
}